{
  "name": "template-typescript",
  "version": "1.0.0",
  "description": "A template repository for TypeScript projects",
  "main": "index.js",
  "repository": "https://github.com/wimpyprogrammer/template-typescript",
  "author": "Drew Keller <drew@wimpyprogrammer.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "npm run clean && babel src -d lib --extensions '.js,.ts'",
    "clean": "rimraf lib/*",
    "lint": "tsc; eslint . --ext .js,.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
